orm: rotate(135deg);\n  visibility: hidden;\n}\n\n.menu .toogler:checked + .menu-container > img:nth-child(2) {\n  visibility: visible;\n}\n\n/*Menu with show and hide*/\n.menu .toogler:checked ~ .menu-container .menu-options_wrap {\n  visibility: visible;\n}\n\n.menu .menu-options_wrap {\n  width: 103vw;\n  height: 8rem;\n  margin-left: 0;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  margin-left: -20px;\n  background-color: white;\n  padding: 10px 0 10px 20px;\n  border-bottom-right-radius: 8px;\n  border-bottom-left-radius: 8px;\n  visibility: hidden;\n}\n\n.menu .menu-options_wrap a {\n  font-size: 1.6rem;\n  font-weight: 500;\n  text-decoration: none;\n  color: black;\n  padding: 10px 0;\n}\n.menu .menu-options_wrap ul li {\n  list-style: none;\n  padding-bottom: 15px;\n}\n\n/*Tablet 768 px minimun\n@media (min-width: 700px) {\n  img {\n    height: 250px;\n  }\n}*/\n</style>\n"]}]}